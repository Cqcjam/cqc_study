<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:property-placeholder
        location="classpath:agency/systemParam.properties"
        ignore-unresolvable="true" />

    <bean id="agencyParamInit" class="com.cqc.unif.agency.common.utils.init.AgencyParamInit">
        <property name="crmSynUrl">
            <value>${crmSynUrl}</value>
        </property>
        <property name="agencyCmccSecretKey">
            <value>${agencyCmccSecretKey}</value>
        </property>
        <property name="agencyCmccId">
            <value>${agencyCmccId}</value>
        </property>
        <property name="agencyCmccName">
            <value>${agencyCmccName}</value>
        </property>
        <property name="agencyCmccSource">
            <value>${agencyCmccSource}</value>
        </property>
        <property name="agencyCmccVersion">
            <value>${agencyCmccVersion}</value>
        </property>
        <property name="agencyCmccIdentityId">
            <value>${agencyCmccIdentityId}</value>
        </property>
        <property name="openIdServer">
            <value>${openIdServer}</value>
        </property>
        <property name="CHINANET_IP">
            <value>${CHINANET_IP}</value>
        </property>
        <property name="CHINANET_SECRET">
            <value>${CHINANET_SECRET}</value>
        </property>
    </bean>

</beans>